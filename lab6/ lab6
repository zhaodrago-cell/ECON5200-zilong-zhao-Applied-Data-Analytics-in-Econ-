      "cell_type": "code",
      "source": [
        "population_probs = df['pclass'].value_counts(normalize=True).sort_index()\n",
        "population_probs\n",
        "\n",
        "sample_size = 100\n",
        "bad_sample=df.sample(n=sample_size, random_state=2026)\n",
        "observe = bad_sample['pclass'].value_counts().sort_index().values\n",
        "expected = (sample_size * population_probs).values\n",
        "print(f\"Observed: {observe}\")\n",
        "print(f\"Expected: {expected}\")\n",
        "\n",
        "from scipy.stats import chisquare\n",
        "chi2_stat, p_value=chisquare(f_obs=observe, f_exp=expected)\n",
        "print(\"-\"*40)\n",
        "print(f\"Chi-Square Statistic: {chi2_stat:.2f}\")\n",
        "print(f\"P-Value Statistic: {p_value:.2f}\")\n",
        "if p_value < 0.01:\n",
        "  print(\"CRITICAL FAILURE:Sample Ration Mismatch(SRM) Detected\")\n",
        "else:\n",
        "  print(\"Pass:Variance is within natural limits\")"
      ],
