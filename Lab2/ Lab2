      "cell_type": "code",
      "source": [
        "import matplotlib.pyplot as plt\n",
        "import seaborn as sns\n",
        "import pandas as pd\n",
        "\n",
        "# --- Style ---\n",
        "sns.set_theme(style=\"whitegrid\")\n",
        "plt.figure(figsize=(14, 7))\n",
        "\n",
        "# --- Plot lines ---\n",
        "plt.plot(\n",
        "    df.index, df[\"Nominal_Wage\"],\n",
        "    linestyle=\"--\", color=\"red\", linewidth=2,\n",
        "    label=\"Nominal Paycheck\"\n",
        ")\n",
        "\n",
        "plt.plot(\n",
        "    df.index, df[\"Real_Wage\"],\n",
        "    linestyle=\"-\", color=\"blue\", linewidth=3.5,\n",
        "    label=\"Real Purchasing Power\"\n",
        ")\n",
        "\n",
        "# --- Title + labels ---\n",
        "plt.title(\"The Illusion of Growth: Nominal vs. Real Wages (1964-Present)\", fontsize=16, weight=\"bold\")\n",
        "plt.xlabel(\"Year\")\n",
        "plt.ylabel(\"Wage Level\")\n",
        "\n",
        "# --- Annotation: 2020 spike in Real Wages ---\n",
        "# Find the closest index value to 2020-01-01 and use that as the annotation point\n",
        "spike_date = pd.Timestamp(\"2020-01-01\")\n",
        "closest_2020 = df.index[df.index.get_indexer([spike_date], method=\"nearest\")[0]]\n",
        "spike_value = df.loc[closest_2020, \"Real_Wage\"]\n",
        "\n",
        "plt.annotate(\n",
        "    \"Pandemic Composition Effect\",\n",
        "    xy=(closest_2020, spike_value),\n",
        "    xytext=(closest_2020 - pd.DateOffset(years=6), spike_value * 1.08),\n",
        "    arrowprops=dict(arrowstyle=\"->\", lw=2),\n",
        "    fontsize=12,\n",
        "    ha=\"left\"\n",
        ")\n",
        "\n",
        "# --- Finishing touches ---\n",
        "plt.legend(frameon=True)\n",
        "plt.tight_layout()\n",
        "plt.show()\n"
      ],
