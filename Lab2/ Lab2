      "cell_type": "code",
      "source": [
        "import matplotlib.pyplot as plt\n",
        "import pandas as pd\n",
        "\n",
        "# Large figure\n",
        "plt.figure(figsize=(14, 7))\n",
        "\n",
        "# Plot both series\n",
        "plt.plot(df.index, df[\"Nominal_Wage\"], color=\"red\", linestyle=\"--\", linewidth=2, label=\"Nominal_Wage\")\n",
        "plt.plot(df.index, df[\"Real_Wage\"], color=\"blue\", linestyle=\"-\", linewidth=2.5, label=\"Real_Wage\")\n",
        "\n",
        "# Title + labels\n",
        "plt.title(\"The Illusion of Growth (1964-Present)\", fontsize=16, fontweight=\"bold\")\n",
        "plt.xlabel(\"Year\")\n",
        "plt.ylabel(\"Wage Level\")\n",
        "\n",
        "# --- Annotate the 2020 spike in Real_Wage ---\n",
        "# Uses nearest date to 2020-01-01 (works for monthly/quarterly/etc. DatetimeIndex)\n",
        "spike_date = pd.Timestamp(\"2020-01-01\")\n",
        "closest_2020 = df.index[df.index.get_indexer([spike_date], method=\"nearest\")[0]]\n",
        "spike_value = df.loc[closest_2020, \"Real_Wage\"]\n",
        "\n",
        "plt.annotate(\n",
        "    \"Pandemic Composition Effect\",\n",
        "    xy=(closest_2020, spike_value),\n",
        "    xytext=(closest_2020 - pd.DateOffset(years=6), spike_value * 1.08),\n",
        "    arrowprops=dict(arrowstyle=\"->\", linewidth=2),\n",
        "    fontsize=12\n",
        ")\n",
        "\n",
        "# Legend + layout\n",
        "plt.legend()\n",
        "plt.tight_layout()\n",
        "plt.show()\n"
      ],
