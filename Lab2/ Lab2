    {
      "cell_type": "code",
      "source": [
        "import matplotlib.pyplot as plt\n",
        "import pandas as pd\n",
        "\n",
        "# 1) Fetch ECI Wage series from FRED\n",
        "eci = fred.get_series(\"ECIWAG\")\n",
        "eci = pd.Series(eci)\n",
        "eci.index = pd.to_datetime(eci.index)\n",
        "eci = eci.sort_index()\n",
        "\n",
        "# Ensure wages is clean + datetime indexed\n",
        "wages = wages.copy()\n",
        "wages.index = pd.to_datetime(wages.index)\n",
        "wages = wages.sort_index()\n",
        "\n",
        "# 2) Filter both series from 2015-01-01 onward\n",
        "start_date = \"2015-01-01\"\n",
        "wages_2015 = wages.loc[start_date:]\n",
        "eci_2015 = eci.loc[start_date:]\n",
        "\n",
        "# Align dates (keep only common index so rebasing/plotting matches perfectly)\n",
        "common_idx = wages_2015.index.intersection(eci_2015.index)\n",
        "wages_2015 = wages_2015.loc[common_idx]\n",
        "eci_2015 = eci_2015.loc[common_idx]\n",
        "\n",
        "# 3) Rebase both to 100\n",
        "wages_rebased = (wages_2015 / wages_2015.iloc[0]) * 100\n",
        "eci_rebased   = (eci_2015   / eci_2015.iloc[0]) * 100\n",
        "\n",
        "# 4) Plot both on one chart\n",
        "plt.figure(figsize=(14, 7))\n",
        "\n",
        "plt.plot(\n",
        "    wages_rebased.index, wages_rebased,\n",
        "    color=\"red\", linestyle=\"--\", linewidth=2,\n",
        "    label=\"Standard Average - Biased\"\n",
        ")\n",
        "\n",
        "plt.plot(\n",
        "    eci_rebased.index, eci_rebased,\n",
        "    color=\"green\", linestyle=\"-\", linewidth=3.5,\n",
        "    label=\"ECI - Fixed Composition\"\n",
        ")\n",
        "\n",
        "plt.title(\"Wage Growth Comparison (Rebased to 100, 2015-Present)\", fontsize=16, fontweight=\"bold\")\n",
        "plt.xlabel(\"Year\")\n",
        "plt.ylabel(\"Index (2015=100)\")\n",
        "plt.legend()\n",
        "plt.tight_layout()\n",
        "\n",
        "# 5) Annotate divergence around 2020\n",
        "spike_date = pd.Timestamp(\"2020-01-01\")\n",
        "closest_2020 = wages_rebased.index[wages_rebased.in_]()\n"
      ],
